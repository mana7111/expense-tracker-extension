{
    "manifest_version": 3,
    "name": "Expense Tracker",
    "version": "1.0",
    "description": "Track your expenses easily.",
    "permissions": ["storage", "alarms"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icons/icon128.png"
    },
    "icons": {
     
      "128": "icons/icon128.png"
    },
    "options_ui": {
      "page": "options.html",
      "open_in_tab": true
    }
  }
  
  